<!DOCTYPE html>
<html>
    <head>
       {% block stylesheet %} <link href="{{ asset('assets/css/style.css') }}" type="text/css" rel="stylesheet" /> {% endblock %}
    </head>
    <body>
        <header>
            <h1><a href="{{ path('homepage') }}">Guestbook</a></h1>
            <ul>
            {% for conference in conferences %}
                <li><a href="{{ path('conference', { slug: conference.slug }) }}">{{ conference }}</a></li>
           {% endfor %}
            </ul>
        </header>
        {% block body %}{% endblock %}
         {% block javascript %} <script src="{{ asset('assets/js/app.js') }}"></script>
{% endblock %}
    </body>
</html>